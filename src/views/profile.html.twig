{% extends "layout.html.twig" %}

{% block title %}Profil de {{ user.firstname }} {{ user.lastname }}{% endblock %}

{% block content %}
    <h1>Profil de {{ user.firstname }} {{ user.lastname }}</h1>

    <p>
        Email: {{ user.email }}
    </p>
    <p>
        Login: {{ user.username }}
    </p>
{% endblock %}